{
    "name": "phpro/annotated-cache",
    "description": "This package makes it easy to configure the caching of your services with annotations.",
    "keywords": ["PSR6", "cache", "annotations", "proxy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toon.verwerft@phpro.be"
        }
    ],
    "require": {
        "php": "~7.0",
        "doctrine/annotations": "^1.2",
        "doctrine/collections": "^1.3",
        "cache/adapter-common": "^0.3.0",
        "cache/taggable-cache": "^0.4.0",
        "ocramius/proxy-manager": "^2.0",
        "psr/cache": "^1.0",
        "symfony/expression-language": "^3.0|^2.6"
    },
    "require-dev": {
        "cache/array-adapter": "^0.4",
        "phpro/grumphp": "^0.8",
        "phpunit/phpunit": "^5.3",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-0": {
            "Phpro\\AnnotatedCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "PhproTest\\AnnotatedCache\\": "tests"
        }
    },
    "suggest": {
        "cache/cache": "This package provides a set of PSR6 pool adapters and a taggable implementation."
    }
}
